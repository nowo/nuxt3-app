{
    "prettier.enable": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "files.associations": {
        "*.css": "postcss"
    },
    "cSpell.words": [
        "alipay",
        "antfu",
        "Attributify",
        "breakline",
        "defu",
        "iconify",
        "nuxi",
        "nuxt",
        "nuxtjs",
        "pinia",
        "pnpm",
        "stylelint",
        "swiper",
        "unjs",
        "Uno",
        "unocss",
        "Vite",
        "vueuse"
    ],
    "i18n-ally.localesPaths": [
        "lang"
    ],
    "i18n-ally.sourceLanguage": "cn"
}